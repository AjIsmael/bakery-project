<table>
  <thead>
    <th scope="col">#</th>
    <th scope="col">Type</th>
    <th scope="col">Quantity</th>
    <th scope="col">Price</th>
  </thead>
  <tbody>
    <% for x in @order %>
    <tr>
      <td class="centered-td" scope="row">1</td>
      <td><%= x[0] %></td>
      <td class="centered-td"><%= x[1] %> </td>
      <td class="centered-td"><%= x[2]*x[1] %></td>
    </tr>
    <%end %>
  </tbody>
</table>
